{
  "tabWidth": 2,
  "useTabs": false,
  "parser": "babel",
  "htmlWhitespaceSensitivity": "ignore",
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser":"html"
      }
    },
        {
      "files": "*.css",
      "options": {
        "parser":"css"
      }
    }
  ],
  "singleQuote": true,
  "semi": false,
  "bracketSpacing": true
}
